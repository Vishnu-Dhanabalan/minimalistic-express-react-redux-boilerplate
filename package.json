{
  "name": "relexpack",
  "version": "1.0.0",
  "author": "Vishnu Vardhan Dhanabalan",
  "description": "React Electron Express Webpack boilerplate",
  "main": "startelectron.js",
  "scripts": {
    "dev": "rm -rf dist && webpack --mode development --config configs/webpack.config.server.js && webpack --mode development --config configs/webpack.config.dev.js && node ./dist/server.js",
    "prod": "rm -rf dist && webpack --mode production --config configs/webpack.config.server.js && webpack --mode production --config configs/webpack.config.prod.js"
  },
  "repository": "https://github.com/Vishnu-Dhanabalan/rexpack.git",
  "keywords": [
    "react",
    "express",
    "webpack",
    "boilerplate"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  }
}
